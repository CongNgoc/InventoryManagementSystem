<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="inventory.model.InvoiceDetail" table="INVOICE_DETAIL" schema="QLKHO_ADMIN">
        <id name="inDeId">
            <column name="IN_DE_ID" sql-type="number(5)" precision="5"/>
        </id>
        <property name="productId">
            <column name="PRODUCT_ID" sql-type="number(5)" precision="5"/>
        </property>
        <property name="invoiceId">
            <column name="INVOICE_ID" sql-type="number(5)" precision="5"/>
        </property>
        <property name="quanity">
            <column name="QUANITY" sql-type="number(5)" precision="5"/>
        </property>
        <property name="activeFlag">
            <column name="ACTIVE_FLAG" sql-type="number(1)" precision="1"/>
        </property>
        <property name="createDate">
            <column name="CREATE_DATE" sql-type="date"/>
        </property>
        <property name="updateDate">
            <column name="UPDATE_DATE" sql-type="date"/>
        </property>
    </class>
</hibernate-mapping>