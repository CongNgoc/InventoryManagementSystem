<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="inventory.model.Menu" table="MENU" schema="QLKHO_ADMIN">
        <id name="menuId" column="MENU_ID"/>
        <property name="menuName" column="MENU_NAME"/>
        <property name="url" column="URL"/>
        <property name="menuIndex" column="MENU_INDEX"/>
        <property name="activeFlag" column="ACTIVE_FLAG"/>
        <property name="createDate" column="CREATE_DATE"/>
        <property name="updateDate" column="UPDATE_DATE"/>
    </class>
</hibernate-mapping>