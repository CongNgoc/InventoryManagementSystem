<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="inventory.model.UserRole" table="USER_ROLE" schema="QLKHO_ADMIN">
        <id name="userRoleId">
            <column name="USER_ROLE_ID" sql-type="number(5)" precision="5"/>
        </id>
        <property name="userId">
            <column name="USER_ID" sql-type="number(5)" precision="5"/>
        </property>
        <property name="roleId">
            <column name="ROLE_ID" sql-type="number(5)" precision="5"/>
        </property>
        <property name="activeFlag">
            <column name="ACTIVE_FLAG" sql-type="number(1)" precision="1"/>
        </property>
        <property name="createDate">
            <column name="CREATE_DATE" sql-type="date"/>
        </property>
        <property name="updateDate">
            <column name="UPDATE_DATE" sql-type="date"/>
        </property>
    </class>
</hibernate-mapping>